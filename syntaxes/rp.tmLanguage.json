{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "rp",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#comment"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.rp",
				"match": "\\b(package|import|readable|writeable|callable|const|bool|byte|char|int2|int4|int8|float4|float8|string|enum|class|deed|this|super|null|any|if|else|switch|do|while|for|break|throw|throws|try|catch|finally|return)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.rp",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.rp",
					"match": "\\\\."
				}
			]
		},
		"comment": {
			"name": "comment.quoted.double.rp",
			"begin": "/\\*",
			"end": "\\*/",
			"patterns": [
				{
					"name": "constant.character.escape.rp",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.rap"
}